<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.test.cct2.resource.cct2.mopp.Cct2MetaInformation" id="org.emftext.test.cct2.resource.cct2.mopp.Cct2MetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.test.cct2.resource.cct2.ui.Cct2Editor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="cct2" icon="icons/editor_icon.gif" id="org.emftext.test.cct2.resource.cct2.ui.Cct2Editor" name="EMFText cct2 Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.test.cct2.resource.cct2.ui.Cct2PreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Cct2 Text Editor" id="org.emftext.test.cct2.resource.cct2.ui.Cct2PreferencePage" class="org.emftext.test.cct2.resource.cct2.ui.Cct2PreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.test.cct2.resource.cct2.ui.Cct2SyntaxColoringPreferencePage" class="org.emftext.test.cct2.resource.cct2.ui.Cct2SyntaxColoringPreferencePage" category="org.emftext.test.cct2.resource.cct2.ui.Cct2PreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.test.cct2.resource.cct2.ui.Cct2BracketPreferencePage" class="org.emftext.test.cct2.resource.cct2.ui.Cct2BracketPreferencePage" category="org.emftext.test.cct2.resource.cct2.ui.Cct2PreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.test.cct2.resource.cct2.ui.Cct2OccurrencePreferencePage" class="org.emftext.test.cct2.resource.cct2.ui.Cct2OccurrencePreferencePage" category="org.emftext.test.cct2.resource.cct2.ui.Cct2PreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.test.cct2.resource.cct2.ui.Cct2NewFileWizard" id="org.emftext.test.cct2.resource.cct2.ui.Cct2NewFileWizard" name="EMFText .cct2 file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.test.cct2.resource.cct2.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.test.cct2.resource.cct2.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.test.cct2.resource.cct2.mopp.Cct2ResourceFactoryDelegator" type="cct2">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.test.cct2.resource.cct2.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
