	</extension>
	
	<extension id="org.emftext.sdk.concretesyntax.resource.cs.nature" name="cs nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.sdk.concretesyntax.resource.cs.mopp.CsNature" />
