<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.emftext.test.puteverywhere.resource.gluetest.GluetestResourceFactoryImpl"
            type="gluetest">
      </parser>
   </extension>

   <extension
         point="org.emftext.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.emftext.org/test/puteverywhere"
            csName="gluetest"
            csDefinition="org.emftext.test.puteverywhere/metamodel/glue.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.emftext.filetype"
            file-extensions="gluetest">
      </file-association>
   </extension>

</plugin>
