<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichMetaInformation" id="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.customsandwich.resource.customsandwich.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.customsandwich.resource.customsandwich.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.customsandwich.resource.customsandwich.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.customsandwich.resource.customsandwich.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.customsandwich.resource.customsandwich.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.nature" name="customsandwich nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.customsandwich.resource.customsandwich.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.customsandwich.resource.customsandwich.builder" name="customsandwich Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.customsandwich.resource.customsandwich.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichResourceFactoryDelegator" type="customsandwich">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.customsandwich.resource.customsandwich.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
</plugin>
