<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.modelquery.resource.modelquery.mopp.ModelqueryMetaInformation" id="org.emftext.language.modelquery.resource.modelquery.mopp.ModelqueryMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.modelquery.resource.modelquery.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.modelquery.resource.modelquery.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.modelquery.resource.modelquery.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.modelquery.resource.modelquery.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.modelquery.resource.modelquery.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.modelquery.resource.modelquery.nature" name="modelquery nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.modelquery.resource.modelquery.mopp.ModelqueryNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.modelquery.resource.modelquery.builder">
		</builder>
		<builder id="org.emftext.language.modelquery.resource.modelquery.taskItemBuilder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.modelquery.resource.modelquery.builder" name="modelquery Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.modelquery.resource.modelquery.mopp.ModelqueryBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.modelquery.resource.modelquery.taskItemBuilder" name="modelquery Task Item Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.modelquery.resource.modelquery.mopp.ModelqueryTaskItemBuilder">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.modelquery.resource.modelquery.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.modelquery.resource.modelquery.mopp.ModelqueryResourceFactoryDelegator" type="modelquery">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.modelquery.resource.modelquery.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
</plugin>
