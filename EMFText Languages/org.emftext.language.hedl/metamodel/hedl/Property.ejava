epackage hedl;

eclass Property {
	
	getTypeClassname() {
		String mainType = getType().getJavaClassname();
		if (isToMultiplicity()) {
			return "List<" + mainType + ">";
		} else {
			return mainType;
		}
	}

	isToOneReference() {
		if (!isToMultiplicity() && !(getType() instanceof JavaType)) {
			return true;
		} else {
			return false;
		}
	}
}
