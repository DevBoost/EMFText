epackage hedl;

eclass Type {
	
	getJavaClassname() {
		if (this instanceof JavaType) {
			JavaType javaType = (JavaType) this;
			return javaType.getJavaClass().getName();
		} else if (this instanceof NamedElement) {
			NamedElement entity = (NamedElement) this;
			return entity.getName();
		}
		return null;
	}
}
