<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.secprop.resource.text.secprop.mopp.TextSecpropMetaInformation" id="org.emftext.language.secprop.resource.text.secprop.mopp.TextSecpropMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.secprop.resource.text.secprop.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.secprop.resource.text.secprop.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.secprop.resource.text.secprop.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.secprop.resource.text.secprop.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.secprop.resource.text.secprop.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.secprop.resource.text.secprop.nature" name="text.secprop nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.secprop.resource.text.secprop.mopp.TextSecpropNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.secprop.resource.text.secprop.builder">
		</builder>
		<builder id="org.emftext.language.secprop.resource.text.secprop.taskItemBuilder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.secprop.resource.text.secprop.builder" name="text.secprop Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.secprop.resource.text.secprop.mopp.TextSecpropBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.secprop.resource.text.secprop.taskItemBuilder" name="text.secprop Task Item Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.secprop.resource.text.secprop.mopp.TextSecpropTaskItemBuilder">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.secprop.resource.text.secprop.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.emftext.language.secprop.resource.additional_extension_parser">
		<parser class="org.emftext.language.secprop.resource.text.secprop.mopp.TextSecpropResourceFactory" type="text">
		</parser>
	</extension>
	
</plugin>
