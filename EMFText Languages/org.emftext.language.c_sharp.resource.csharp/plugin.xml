<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.c_sharp.resource.csharp.mopp.CsharpMetaInformation" id="org.emftext.language.c_sharp.resource.csharp.mopp.CsharpMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.c_sharp.resource.csharp.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.c_sharp.resource.csharp.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.c_sharp.resource.csharp.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.c_sharp.resource.csharp.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.c_sharp.resource.csharp.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.c_sharp.resource.csharp.nature" name="csharp nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.c_sharp.resource.csharp.mopp.CsharpNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.c_sharp.resource.csharp.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.c_sharp.resource.csharp.builder" name="csharp Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.c_sharp.resource.csharp.mopp.CsharpBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.c_sharp.resource.csharp.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.c_sharp.resource.csharp.mopp.CsharpResourceFactoryDelegator" type="csharp">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.c_sharp.resource.csharp.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
</plugin>
