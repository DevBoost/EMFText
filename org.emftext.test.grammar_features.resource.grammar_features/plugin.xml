<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.test.grammar_features.resource.grammar_features.Grammar_featuresResourceFactory" type="grammar_features">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.test.grammar_features.resource.grammar_features.Grammar_featuresMetaInformation" id="org.emftext.test.grammar_features.resource.grammar_features.Grammar_featuresMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="grammar_features">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.test.grammar_features.resource.grammar_features.Grammar_featuresNewFileWizard" id="org.emftext.test.grammar_features.resource.grammar_features.Grammar_featuresNewFileWizard" name="EMFText .grammar_features file">
		</wizard>
	</extension>
	
</plugin>
