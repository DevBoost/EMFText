<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.test.grammar_features.resource.grammar_features.mopp.Grammar_featuresMetaInformation" id="org.emftext.test.grammar_features.resource.grammar_features.mopp.Grammar_featuresMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="grammar_features" icon="icons/editor_icon.gif" id="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresEditor" name="EMFText grammar_features Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Grammar_features Text Editor" id="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresPreferencePage" class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresSyntaxColoringPreferencePage" class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresSyntaxColoringPreferencePage" category="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresBracketPreferencePage" class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresBracketPreferencePage" category="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresOccurrencePreferencePage" class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresOccurrencePreferencePage" category="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresNewFileWizard" id="org.emftext.test.grammar_features.resource.grammar_features.ui.Grammar_featuresNewFileWizard" name="EMFText .grammar_features file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.test.grammar_features.resource.grammar_features.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.test.grammar_features.resource.grammar_features.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.test.grammar_features.resource.grammar_features.mopp.Grammar_featuresResourceFactoryDelegator" type="grammar_features">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.test.grammar_features.resource.grammar_features.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
